<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.model.Customer" table="customer">
		<id name="custID" column="custid"></id>
		<property name="custName" column="custname" />
		<property name="custUserName" column="custusername"></property>
		<property name="custPassword" column="custpassword"></property>		
	</class>
</hibernate-mapping>